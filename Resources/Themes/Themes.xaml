<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WindowBackground">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="InputBorderBrush">#CACACA</SolidColorBrush>
    <SolidColorBrush x:Key="InputForeground">#0D062D</SolidColorBrush>
       
    
    <SolidColorBrush x:Key="ThemeBrush">#0076F6</SolidColorBrush>
    <SolidColorBrush x:Key="ThemeForeBrush">#51A2F9</SolidColorBrush>

    <Color x:Key="ThemeColor">#0076F6</Color>
    <Color x:Key="ThemeForeColor">#51A2F9</Color>

    <SolidColorBrush x:Key="ThemeBackground">#F3F3F3</SolidColorBrush>

</ResourceDictionary>